<!-- Page Header -->
<header class="masthead" style="background-image: url('img/Covid\ Near\ Me.png')">
<div class="overlay"></div>
<div class="container">
  <div class="row">
    <div class="col-lg-8 col-md-10 mx-auto">
      <div class="site-heading">
        <h1 style="color: aqua;">Edit</h1>
      </div>
    </div>
  </div>
</div>
</header>


<section class="margin-5-25">
<form class="" action="/posts/<%= postId %>/?_method=PUT" method="post">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="postTitle" value="<%= title %>">
    <label>Picture</label>
    <input class="form-control" type="url" name="picture" value="<%= picture %>">
    <label>Content</label>
    <textarea class="form-control" name="postBody" rows="20" cols="30"><%= content %></textarea>
    <label>Author</label>
    <textarea class="form-control" name="author" rows="1" cols="30" value="<%= author %>"><%= author %></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Edit</button>
</form>
</section>


<%- include("partials/footerNotHome"); -%>
